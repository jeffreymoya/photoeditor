  "required_fields": {
    "correlationId": "Traces request across services",
    "requestId": "API Gateway request ID or Lambda request ID",
    "jobId": "Business entity identifier (job/upload ID)",
    "userId": "User identifier (if authenticated)",
    "function": "Lambda function name",
    "env": "Environment name (dev/stage/prod)",
    "version": "Lambda version or git commit SHA"
