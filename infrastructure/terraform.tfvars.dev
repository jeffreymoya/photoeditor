# Development environment configuration
project_name = "photo-editor"
environment  = "dev"
region       = "us-east-1"

# Development-specific settings
budget_limit = 50.0  # Lower budget limit for dev
budget_alert_emails = ["dev-team@example.com"]

# Lambda configuration for development
lambda_reserved_concurrency = 5  # Lower concurrency for dev

# Monitoring settings
enable_detailed_monitoring = true
log_retention_days        = 7  # Shorter retention for dev

# Security settings - use development KMS key
kms_deletion_window = 7  # Shorter deletion window for dev

# DynamoDB settings
dynamodb_point_in_time_recovery = false  # Disable for cost savings in dev

# S3 settings
enable_s3_access_logging = false  # Disable for cost savings in dev

# Development feature flags
enable_debug_logging = true