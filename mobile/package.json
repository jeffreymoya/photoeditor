{
  "name": "photoeditor-mobile",
  "version": "1.0.0",
  "description": "React Native mobile app for photo editing",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:eas": "eas build --platform all",
    "build": "echo 'Mobile build verification - typecheck and lint in qa:static' && exit 0",
    "submit": "eas submit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf node_modules && pnpm install",
    "qa:static": "pnpm run typecheck && pnpm run lint",
    "qa:dependencies": "echo 'Mobile: dependencies checked at root level'",
    "qa:dead-exports": "ts-prune -p tsconfig.json || true",
    "qa:duplication": "echo 'Mobile: duplication checked at root level'",
    "qa": "pnpm run qa:static && pnpm run test"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@photoeditor/shared": "*",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-native-camera-roll/camera-roll": "^7.4.0",
    "@react-native-community/netinfo": "^11.2.1",
    "@react-native-masked-view/masked-view": "^0.3.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@xstate/react": "^3.2.0",
    "cockatiel": "^3.2.1",
    "expo": "~50.0.0",
    "expo-av": "~13.10.4",
    "expo-camera": "~14.1.0",
    "expo-constants": "~15.4.0",
    "expo-dev-client": "~3.3.0",
    "expo-device": "~5.9.4",
    "expo-file-system": "~16.0.0",
    "expo-image": "~1.10.0",
    "expo-image-manipulator": "^14.0.7",
    "expo-image-picker": "~14.7.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-media-library": "~15.9.0",
    "expo-notifications": "~0.27.0",
    "expo-status-bar": "~1.11.0",
    "lucide-react-native": "^0.547.0",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "~4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-redux": "^9.1.0",
    "xstate": "^4.38.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/runtime": "^7.23.6",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.8",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^61.1.5",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~50.0.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0"
  },
  "private": true,
  "keywords": [
    "react-native",
    "expo",
    "photo-editor",
    "mobile"
  ],
  "author": "",
  "license": "MIT"
}
