{
  "name": "photoeditor-mobile",
  "version": "1.0.0",
  "description": "React Native mobile app for photo editing",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:eas": "eas build --platform all",
    "build": "echo 'Mobile build verification - typecheck and lint in qa:static' && exit 0",
    "submit": "eas submit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "sb-rn-get-stories && react-native start",
    "storybook:generate": "sb-rn-get-stories",
    "storybook:web": "storybook dev -p 6006",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "clean": "rm -rf node_modules && pnpm install",
    "qa:static": "pnpm run typecheck && pnpm run lint",
    "qa:dependencies": "echo 'Mobile: dependencies checked at root level'",
    "qa:dead-exports": "ts-prune -p tsconfig.json || true",
    "qa:duplication": "echo 'Mobile: duplication checked at root level'",
    "qa": "pnpm run qa:static && pnpm run test"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@photoeditor/shared": "*",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-camera-roll/camera-roll": "^7.4.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@shopify/flash-list": "2",
    "@tamagui/button": "^1.0.0",
    "@tamagui/core": "^1.0.0",
    "@tamagui/input": "^1.0.0",
    "@tamagui/stacks": "^1.0.0",
    "@tamagui/text": "^1.0.0",
    "@xstate/react": "^3.2.0",
    "cockatiel": "^3.2.1",
    "expo": "~53.0.0",
    "expo-av": "~14.0.7",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.29",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-image": "~1.13.0",
    "expo-image-manipulator": "^12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-media-library": "~16.0.5",
    "expo-notifications": "~0.28.19",
    "expo-router": "~4.0.0",
    "expo-status-bar": "~1.12.1",
    "lucide-react-native": "^0.547.0",
    "nativewind": "5.0.0-preview.2",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "~4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-worklets": "~0.5.2",
    "react-redux": "^9.1.0",
    "tailwindcss": "^4.0.0",
    "xstate": "^4.38.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@babel/runtime": "^7.23.6",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-react-native-web": "0.0.23",
    "@storybook/react-native": "^7.6.20",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.8",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.45.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "chromatic": "^11.0.0",
    "eslint": "^9.0.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsdoc": "^61.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "react-dom": "19.0.0",
    "react-native-web": "~0.20.0",
    "react-test-renderer": "19.0.0",
